{
    "compilerOptions": {
        "target": "ESNext",
        "module": "esnext",
        "moduleResolution": "node",
        "noImplicitAny": false,
        "removeComments": true,
        "preserveConstEnums": true,
        "sourceMap": true,
        "forceConsistentCasingInFileNames": true,
        "declaration": true,
        "noEmit": true
    },
    "exclude": ["node_modules"]
}
